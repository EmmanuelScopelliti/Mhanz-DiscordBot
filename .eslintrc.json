{
    "extends": "eslint:recommended",
    "env": {
        "es2020": true,
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2019
    },
    "rules": {
        "no-unused-vars": ["error", { "vars": "local", "args": "none", "ignoreRestSiblings": false }],
		"semi": ["error", "always"]
    }
}